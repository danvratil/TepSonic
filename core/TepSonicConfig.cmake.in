# TepSonicConfig.cmake is generated by CMake from TepSonicConfig.cmake.in.
# Any changed value in this file will be overwritten by CMake.

@PACKAGE_INIT@

get_filename_component(_currentDir ${CMAKE_CURRENT_LIST_FILE} PATH) # The current directory
get_filename_component(rootDir ${_currentDir}/@relInstallDir@ ABSOLUTE) # The install prefix

# set the akonadi version number
set(TEPSONIC_VERSION_MAJOR @TEPSONIC_VERSION_MAJOR@)
set(TEPSONIC_VERSION_MINOR @TEPSONIC_VERSION_MINOR@)
set(TEPSONIC_VERSION_PATCH @TEPSONIC_VERSION_PATCH@)
set(TEPSONIC_VERSION @TEPSONIC_VERSION@)
set(TEPSONIC_VERSION_STRING "@TEPSONIC_VERSION_STRING@")

# set the directories
set_and_check(TEPSONIC_INCLUDE_DIR          "@PACKAGE_INCLUDE_INSTALL_DIR@")
set_and_check(TEPSONIC_LIB_DIR              "@PACKAGE_LIB_INSTALL_DIR@")
set_and_check(TEPSONIC_SHARE_DIR            "@PACKAGE_SHARE_INSTALL_PREFIX@")

# Include directory
set(TEPSONIC_INSTALL_PREFIX "${rootDir}")
set(TEPSONIC_INCLUDE_DIR "@INCLUDE_INSTALL_DIR@")

# import the exported targets (only once)
if(NOT TARGET tepsonic_core)
  include(${_currentDir}/TepSonicCoreTargetsWithPrefix.cmake)
endif()

# Set the library variable
set(TEPSONIC_LIBRARY tepsonic_core)
